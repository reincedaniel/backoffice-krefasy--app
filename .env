# Krefasy Backoffice Environment Variables

# API Configuration
VITE_API_BASE_URL=https://krafasy-credit-api.mayacode.co/api/v1
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Krefasy Backoffice
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Backoffice de Gestão de Créditos e Empréstimos

# Authentication
VITE_token_KEY=token
VITE_USER_DATA_KEY=user_data

# Features
VITE_ENABLE_REAL_TIME_NOTIFICATIONS=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_MAX_FILE_SIZE=10485760

# Development
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false

# External Services
VITE_WEBSOCKET_URL=wss://krafasy-credit-api.mayacode.co/ws
VITE_FILE_STORAGE_URL=https://krafasy-credit-api.mayacode.co/storage
